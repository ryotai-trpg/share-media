{
  "id": "share-media",
  "title": "Share Media",
  "description": "This module allows GMs to quickly share media.",
  "version": "#{VERSION}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "url": "https://github.com/mech-tools/share-media",
  "readme": "https://github.com/mech-tools/share-media/blob/master/README.md",
  "changelog": "https://github.com/mech-tools/share-media/releases",
  "bugs": "https://github.com/mech-tools/share-media/issues",
  "compatibility": {
    "minimum": "13",
    "verified": "13",
    "maximum": "13"
  },
  "authors": [
    {
      "name": "Maxime",
      "url": "https://github.com/mech-tools",
      "discord": "Maxime1562"
    }
  ],
  "esmodules": ["./share-media.mjs"],
  "styles": ["./share-media.css"],
  "documentTypes": {
    "RegionBehavior": {
      "ShareRegion": {}
    }
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    },
    {
      "lang": "fr",
      "name": "Fran√ßais",
      "path": "languages/fr.json"
    }
  ],
  "flags": {
    "hotReload": {
      "extensions": ["css", "hbs", "json"],
      "paths": ["share-media.css", "templates/", "languages/"]
    }
  },
  "socket": true
}
